<h1>Show Upload</h1>

  <ul>
    <% unless @upload.image.nil? %>
    <li><%= image_tag @upload.image %></li>
    <%end%>
    <li><%= @upload.information %></li>

  </ul>



<div class="controls">
  <%= link_to "Edit Upload", edit_upload_path(@upload) %>
  <%= link_to "Delete Upload", @upload, :method => 'delete', :data => {:confirm => 'Are you sure?'} %>

</div>
